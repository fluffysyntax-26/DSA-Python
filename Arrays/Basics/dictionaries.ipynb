    "cities = [\"Bengaluru\", \"Mumbai\", \"Delhi\", \"Chennai\", \"Kolkata\"]\n",
    "temperatures_celsius = [25, 30, 32, 28, 31]\n",
    "\n",
    "weather = {city:temp for city, temp in zip(cities, temperatures_celsius)}\n",
    "print(weather)\n",
